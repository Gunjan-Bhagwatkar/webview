<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#034447"
    tools:context=".MainActivity">

    <EditText
        android:id="@+id/eturl"
        android:layout_width="300dp"
        android:layout_height="41dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:layout_marginBottom="24dp"
        android:background="@drawable/edittext"
        android:ems="10"
        android:hint="   @string/searchurl"
        android:inputType="textPersonName"
        android:textColor="#0C0000"
        android:textColorHint="#3E1A1A"
        app:layout_constraintBottom_toTopOf="@+id/btninsta"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/btnback"
        android:layout_width="60dp"
        android:layout_height="37dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:backgroundTint="#5BABEA"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mywebview"
        app:srcCompat="@drawable/ic_baseline_arrow_back_24" />

    <WebView
        android:id="@+id/mywebview"
        android:layout_width="397dp"
        android:layout_height="563dp"
        android:layout_marginTop="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btntwit" />

    <ImageButton
        android:id="@+id/btngo"
        android:layout_width="60dp"
        android:layout_height="39dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/edittext"
        android:backgroundTint="#FFFCFC"
        app:layout_constraintStart_toEndOf="@+id/eturl"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ss" />

    <ImageButton
        android:id="@+id/btnfor"
        android:layout_width="60dp"
        android:layout_height="37dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="8dp"
        android:backgroundTint="#5BABEA"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mywebview"
        app:srcCompat="@drawable/ic_baseline_arrow_forward_24" />

    <ImageButton
        android:id="@+id/btnhome"
        android:layout_width="73dp"
        android:layout_height="37dp"
        android:layout_marginStart="92dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="92dp"
        android:layout_marginBottom="8dp"
        android:backgroundTint="#5BABEA"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/btnfor"
        app:layout_constraintStart_toEndOf="@+id/btnback"
        app:layout_constraintTop_toBottomOf="@+id/mywebview"
        app:srcCompat="@drawable/ic_baseline_home_24" />

    <ImageButton
        android:id="@+id/btninsta"
        android:layout_width="54dp"
        android:layout_height="50dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="50dp"
        android:backgroundTint="#34495E"
        app:layout_constraintEnd_toStartOf="@+id/btnfb"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/eturl"
        app:srcCompat="@drawable/instagram" />

    <ImageButton
        android:id="@+id/btnfb"
        android:layout_width="54dp"
        android:layout_height="50dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="50dp"
        android:backgroundTint="#34495E"
        app:layout_constraintEnd_toStartOf="@+id/btntwit"
        app:layout_constraintStart_toEndOf="@+id/btninsta"
        app:layout_constraintTop_toBottomOf="@+id/eturl"
        app:srcCompat="@drawable/fac" />

    <ImageButton
        android:id="@+id/btntwit"
        android:layout_width="54dp"
        android:layout_height="50dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="50dp"
        android:backgroundTint="#34495E"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btnfb"
        app:layout_constraintTop_toBottomOf="@+id/eturl"
        app:srcCompat="@drawable/twi" />

</androidx.constraintlayout.widget.ConstraintLayout>